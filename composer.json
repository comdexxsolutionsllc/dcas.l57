{
    "name": "fiberhopllc_dcas",
    "description": "FiberHop LLC - DCAS - Datacenter Automation Suite",
    "keywords": [
        "framework",
        "laravel",
        "dcas",
        "web hosting",
        "datacenter",
        "fiberhop"
    ],
    "license": "Closed Source",
    "type": "project",
    "require": {
        "appstract/laravel-opcache": "^2.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.5",
        "bpocallaghan/sluggable": "^1.2",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "~1.1",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/flash": "^3.0",
        "laracasts/utilities": "^3.0",
        "laravel-shield/stripe": "^1.0",
        "laravel/cashier": "~7.0",
        "laravel/envoy": "^1.4",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^1.4",
        "laravel/passport": "^7.0",
        "laravel/scout": "^5.0",
        "laravel/tinker": "^1.0",
        "php": "^7.1.3",
        "predis/predis": "^1.1",
        "softonic/laravel-middleware-request-id": "^1.0",
        "spatie/laravel-permission": "*",
        "spatie/laravel-robots-middleware": "^1.0",
        "spatie/laravel-schemaless-attributes": "^1.1",
        "stevenmaguire/laravel-middleware-csp": "^0.1.1",
        "stripe/stripe-php": "*",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "ahuggins/open-on-make": "*",
        "ajthinking/tinx": "^2.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^0.2.2",
        "crestapps/laravel-code-generator": "*",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jlapp/swaggervel": "dev-master",
        "laracasts/generators": "^1.1",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.0",
        "mtrajano/laravel-swagger": "^0.2.0",
        "nunomaduro/collision": "^2.0",
        "phpmd/phpmd": "^2.6",
        "phpspec/phpspec": "^4.3",
        "phpunit/phpunit": "^7.0",
        "stolz/laravel-schema-spy": "^0.1.1",
        "swagger-api/swagger-ui": "^3.13"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "ComdexxSolutionsLLC\\": "vendor_internal/laravel-scout-mysql-driver/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
